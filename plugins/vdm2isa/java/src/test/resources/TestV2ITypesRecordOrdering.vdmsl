module TestV2ITypesRecordOrdering
exports all
definitions

types	

	Time :: 
		r : real
		i : nat
	inv mk_Time(r, -) == r >= 0
	eq a = b == a.r = b.r and a.i = b.i
	ord a < b == a.r < b.r or (a.r = b.r and a.i < b.i);
	
	User ::
		time : Time
		endtime: Time;
	
functions
	f: Time * Time -> bool
	f(t1, t2) == t1 >= t2;
	
	g: User -> bool
	g(mk_User(bt, et)) == et >= bt;
	
	g': User -> bool
	g'(u) == u.endtime >= u.time;
	
end TestV2ITypesRecordOrdering
