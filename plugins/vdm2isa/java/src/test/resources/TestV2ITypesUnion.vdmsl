module TestV2ITypesUnion
exports all
definitions

types

	--quotes *must* be part of a union (and not dangling) we need their unique names on literals
	TQuoteUnion = <A> | <B> | <C>;
	
	--other unions are okay
	TBasicUnion = nat | int;
	
	--you can mix unions
	TBasicQuoteUnion = TBasicUnion | TQuoteUnion; 
	
functions

	f1: TQuoteUnion -> bool
	f1(qu) == qu <> <A>;

	f2: TBasicUnion -> int
	f2(bu) == bu;
		
end TestV2ITypesUnion

